  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build --ignore generator --ignore documentation",
    "format": "prettier --write \"packages/*/**/*.ts\" \"packages/*/**/*.tsx\"",
    "lint": "tslint -c tslint.json -p tsconfig.json -e 'packages/generator/**/*.ts' -e 'packages/documentation/**/*.*'",
    "start": "cd packages/documentation && npm run start",
    "dev": "npm-run-all -p \"watch\" \"start\"",
    "watch": "lerna run watch --ignore examples"
  },
