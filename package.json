{
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "scripts": {
    "build:base-deps": "lerna run --parallel build --scope @react-md/theme --scope @react-md/typography --scope @react-md/transition --scope @react-md/utils --scope @react-md/elevation --scope @react-md/portal",
    "build:base": "lerna run --parallel build --scope @react-md/states --scope @react-md/listeners --scope @react-md/icon --scope @react-md/a11y --scope @react-md/link --scope @react-md/overlay",
    "build:remaining": "lerna run build --scope @react-md/button --scope @react-md/material-icons --scope @react-md/list --scope @react-md/tree-view --scope @react-md/tooltip --scope @react-md/sheet --scope @react-md/app-bar --scope react-md",
    "build": "npm run build:base-deps && npm run build:base && npm run build:remaining",
    "format": "prettier --write \"packages/*/**/*.ts\" \"packages/*/**/*.tsx\"",
    "lint:styles": "sass-lint -v",
    "start": "cd packages/documentation && npm run start",
    "dev": "npm-run-all -p \"watch\" \"start\"",
    "watch": "lerna run watch --ignore doc-generator --ignore examples",
    "docs": "lerna run docs --ignore examples --ignore doc-generator",
    "toc": "./packages/doc-generator/src/index.js --no-proptypes --no-sassdoc"
  },
  "homepage": "https://github.com/mlaursen/react-md#readme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/commander": "^2.12.2",
    "@types/cpx": "^1.5.0",
    "@types/enzyme": "^3.1.14",
    "@types/fs-extra": "^5.0.4",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.6",
    "@types/lodash": "^4.14.117",
    "@types/memoize-one": "^3.1.1",
    "@types/node": "^10.12.0",
    "@types/node-sass": "^3.10.32",
    "@types/prop-types": "^15.5.6",
    "@types/reach__router": "^1.2.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router": "^4.0.32",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/react-transition-group": "^2.0.14",
    "babel-eslint": "^8.2.6",
    "commander": "^2.19.0",
    "cpx": "^1.5.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "fs-extra": "^7.0.0",
    "jest": "^23.6.0",
    "lerna": "^3.4.3",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sassdoc": "^2.5.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  }
}
