{
  "name": "@react-md/material-icons",
  "version": "2.0.0-alpha-1",
  "description": "This package is just a simple wrapper for using material icons within react-md as either font icons or svg icons.",
  "scripts": {
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "build:modules": "tsc -p tsconfig.modules.json",
    "build:definitions": "tsc -p tsconfig.modules.json -d --declarationDir ./types",
    "build": "npm-run-all -p \"build:commonjs\" \"build:modules\" \"build:definitions\"",
    "prettier": "prettier --write src/*.tsx src/*.ts",
    "icons": "./createIcons.js",
    "clean": "rimraf es lib types",
    "prebuild": "npm run clean",
    "update-definitions": "rimraf types && npm run build:definitions",
    "watch:definitions": "watch \"npm run update-definitions\" src --filter ../../tsWatchFilter.js",
    "watch": "npm-run-all -p \"build:commonjs -- --watch\" \"build:modules -- --watch\"",
    "watch:with-defs": "npm-run-all -p watch watch:definitions",
    "docs": "doc-generator --no-sassdoc --no-proptypes",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "es/*",
    "lib/*",
    "types/*",
    "svgs/*"
  ],
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": "https://github.com/mlaursen/react-md",
  "keywords": [
    "react-md",
    "material design",
    "react",
    "component",
    "components",
    "icons",
    "svg icon",
    "font icon",
    "material design icons"
  ],
  "license": "MIT",
  "devDependencies": {
    "@react-md/icon": "^2.0.0-alpha-1",
    "bluebird": "^3.5.1",
    "doc-generator": "^2.0.0-alpha-1",
    "download": "^7.0.0",
    "glob": "^7.1.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "tslint": "^4.5.1",
    "typescript": "^3.0.1",
    "unzipper": "^0.9.2",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "@react-md/icon": "^2.0.0-alpha-1"
  }
}
