{
  "name": "react-md",
  "version": "2.0.0-alpha-1",
  "description": "",
  "scripts": {
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "build:modules": "tsc -p tsconfig.json",
    "build": "npm-run-all -p build:commonjs build:modules styles",
    "clean": "rimraf es lib types dist",
    "prebuild": "npm run clean",
    "styles": "cpx \"src/**/*.scss\" dist",
    "watch:styles": "npm run styles -- --watch",
    "watch": "npm-run-all -p \"build:commonjs -- --watch\" \"build:modules -- --watch\" watch:styles",
    "test": "jest src",
    "docs": "doc-generator",
    "lint": "npm-run-all -p lint:styles lint:ts",
    "lint:styles": "sass-lint -c ../../.sass-lint.yml -v",
    "lint:ts": "tslint -p tsconfig.json",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "es/*",
    "lib/*",
    "types/*",
    "dist/*"
  ],
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": "https://github.com/mlaursen/react-md",
  "license": "MIT",
  "dependencies": {
    "@react-md/a11y": "^2.0.0-alpha-1",
    "@react-md/app-bar": "^2.0.0-alpha-1",
    "@react-md/button": "^2.0.0-alpha-1",
    "@react-md/elevation": "^2.0.0-alpha-1",
    "@react-md/icon": "^2.0.0-alpha-1",
    "@react-md/link": "^2.0.0-alpha-1",
    "@react-md/list": "^2.0.0-alpha-1",
    "@react-md/listeners": "^2.0.0-alpha-1",
    "@react-md/overlay": "^2.0.0-alpha-1",
    "@react-md/portal": "^2.0.0-alpha-1",
    "@react-md/sheet": "^2.0.0-alpha-1",
    "@react-md/states": "^2.0.0-alpha-1",
    "@react-md/theme": "^2.0.0-alpha-1",
    "@react-md/tooltip": "^2.0.0-alpha-1",
    "@react-md/transition": "^2.0.0-alpha-1",
    "@react-md/tree-view": "^2.0.0-alpha-1",
    "@react-md/typography": "^2.0.0-alpha-1",
    "@react-md/utils": "^2.0.0-alpha-1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "doc-generator": "^2.0.0-alpha-1",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  }
}
