{
  "name": "@react-md/elevation",
  "version": "2.0.0-alpha-1",
  "description": "This is a small package for react-md that can create the elevation styles in the material design spec.",
  "scripts": {
    "build": "npm-run-all -p \"styles\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "styles": "cpx \"src/**/*.scss\" dist",
    "watch": "npm run styles -- --watch",
    "docs": "doc-generator --no-proptypes",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "files": [
    "dist/*"
  ],
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": "https://github.com/mlaursen/react-md",
  "keywords": [
    "react-md",
    "material design",
    "sass",
    "styles",
    "elevation"
  ],
  "license": "MIT",
  "dependencies": {
    "@react-md/theme": "^2.0.0-alpha-1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "doc-generator": "^2.0.0-alpha-1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "watch": "^1.0.2"
  }
}
