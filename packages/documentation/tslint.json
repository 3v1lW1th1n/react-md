{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": [0],
    "object-literal-key-quotes": [0],
    "ordered-imports": [0],
    "trailing-comma": [0],
    "arrow-parens": [0],
    "semicolon": [0],
    "no-console": [0],
    "no-empty-interface": [0]
  },
  "rulesDirectory": []
}
