{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "target": "es5",
    "module": "commonjs",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "constants/*": ["constants/*"],
      "components/*": ["components/*"],
      "hooks/*": ["hooks/*"],
      "icons/*": ["icons/*"],
      "types/*": ["types/*"],
      "utils/*": ["utils/*"]
    }
  },
  "include": [
    "constants",
    "components",
    "hooks",
    "icons",
    "types",
    "utils"
  ]
}
