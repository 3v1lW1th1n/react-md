{
  "name": "documentation",
  "version": "2.0.0-alpha.2",
  "description": "The documentation site for react-md",
  "private": true,
  "scripts": {
    "prebuild": "npm-run-all copy-readmes base-styles \"sandbox --empty\"",
    "base-styles": "dev-utils doc-styles",
    "typecheck": "tsc --noEmit",
    "add-toc": "dev-utils toc \"+(pages|components)/**/*.md\"",
    "copy-readmes": "dev-utils readmes",
    "sandbox": "dev-utils sandbox",
    "sassdoc": "dev-utils sassdoc",
    "dev": "npm-run-all prebuild start-dev",
    "dev-log": "FORCE_COLOR=true next | cat",
    "build": "next build",
    "start": "next start",
    "start-dev": "next",
    "start-prod": "npm-run-all build start"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": "https://github.com/mlaursen/react-md",
  "license": "MIT",
  "dependencies": {
    "@react-md/app-bar": "^2.0.0-alpha.1",
    "@react-md/avatar": "^2.0.0-alpha.1",
    "@react-md/button": "^2.0.0-alpha.1",
    "@react-md/dialog": "^2.0.0-alpha.2",
    "@react-md/divider": "^2.0.0-alpha.1",
    "@react-md/elevation": "^2.0.0-alpha.1",
    "@react-md/icon": "^2.0.0-alpha.1",
    "@react-md/link": "^2.0.0-alpha.1",
    "@react-md/list": "^2.0.0-alpha.1",
    "@react-md/material-icons": "^2.0.0-alpha.1",
    "@react-md/media": "^2.0.0-alpha.1",
    "@react-md/overlay": "^2.0.0-alpha.1",
    "@react-md/portal": "^2.0.0-alpha.1",
    "@react-md/sheet": "^2.0.0-alpha.1",
    "@react-md/states": "^2.0.0-alpha.1",
    "@react-md/theme": "^2.0.0-alpha.1",
    "@react-md/tooltip": "^2.0.0-alpha.1",
    "@react-md/transition": "^2.0.0-alpha.1",
    "@react-md/tree": "^2.0.0-alpha.2",
    "@react-md/typography": "^2.0.0-alpha.1",
    "@react-md/utils": "^2.0.0-alpha.1",
    "@react-md/wia-aria": "^2.0.0-alpha.2",
    "codesandbox": "^2.1.4",
    "marked": "^0.6.2",
    "mobile-detect": "^1.4.3",
    "next": "^8.1.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@react-md/dev-utils": "^2.0.0-alpha.2",
    "@types/marked": "^0.6.5",
    "@types/next": "^8.0.3",
    "@types/prismjs": "^1.16.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-prismjs": "^1.0.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-preset-env": "^6.6.0",
    "raw-loader": "^2.0.0",
    "typescript": "^3.4.3",
    "webpack": "^4.30.0"
  }
}
