{
  "public/index.html": {
    "content": "<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"utf-8\" />\n    <meta\n      name=\"viewport\"\n      content=\"width=device-width, initial-scale=1, shrink-to-fit=no\"\n    />\n    <meta name=\"theme-color\" content=\"#000000\" />\n    <link\n      rel=\"stylesheet\"\n      href=\"https://fonts.googleapis.com/css?family=Material+Icons|Roboto:400,500,700|Source+Code+Pro\"\n    />\n    <title>Media Example - Forced Aspect Ratio</title>\n  </head>\n  <body>\n    <noscript>You need to enable JavaScript to run this app.</noscript>\n    <div id=\"root\"></div>\n  </body>\n</html>\n",
    "isBinary": false
  },
  "src/index.tsx": {
    "content": "import React from \"react\";\nimport { render } from \"react-dom\";\nimport { StatesConfig } from \"@react-md/states\";\n\nimport \"./styles.scss\";\nimport Demo from \"./Demo\";\n\nconst App = () => (\n  <StatesConfig>\n    <Demo />\n  </StatesConfig>\n);\n\nrender(<App />, document.getElementById(\"root\"));\n",
    "isBinary": false
  },
  "src/styles.scss": {
    "content": "@import '@react-md/media/dist/mixins';\n@import '@react-md/states/dist/mixins';\n@import '@react-md/theme/dist/mixins';\n@import '@react-md/typography/dist/mixins';\n@import '@react-md/utils/dist/mixins';;\n\n@include react-md-utils;\n",
    "isBinary": false
  },
  "package.json": {
    "content": {
      "title": "Media Example - Forced Aspect Ratio",
      "description": "Example from https://react-md.dev/packages/media/demos#forced-aspect-ratio",
      "main": "src/index.tsx",
      "dependencies": {
        "@react-md/media": "next",
        "react": "latest",
        "@react-md/states": "next",
        "@react-md/theme": "next",
        "@react-md/typography": "next",
        "@react-md/utils": "next",
        "react-dom": "latest"
      },
      "devDependencies": {
        "react-scripts": "latest",
        "node-sass": "latest",
        "typescript": "latest",
        "@types/node": "latest",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "@types/jest": "latest"
      },
      "scripts": {
        "start": "react-scripts start"
      }
    },
    "isBinary": false
  },
  ".env": {
    "content": "SASS_PATH=node_modules:src",
    "isBinary": false
  },
  "src/Demo.tsx": {
    "content": "import React, { FunctionComponent } from \"react\";\nimport { MediaContainer } from \"@react-md/media\";\n\nconst ForcedAspectRatio: FunctionComponent = () => (\n  <div className=\"forced-aspect-ratio\">\n    <MediaContainer height={9} width={16} className=\"responsive-item\">\n      <img\n        src=\"https://picsum.photos/400/300?image=3\"\n        role=\"presentation\"\n        alt=\"An image from https://picsum.photos\"\n      />\n    </MediaContainer>\n    <MediaContainer height={1} width={1} className=\"responsive-item\">\n      <img\n        src=\"https://picsum.photos/400/300?image=623\"\n        role=\"presentation\"\n        alt=\"An image from https://picsum.photos\"\n      />\n    </MediaContainer>\n    <MediaContainer height={9} width={16} className=\"responsive-item\">\n      <iframe\n        src=\"https://www.youtube.com/embed/kyAn3fSs8_A\"\n        allowFullScreen\n        frameBorder=\"0\"\n      />\n    </MediaContainer>\n  </div>\n);\n\nexport default ForcedAspectRatio;\n",
    "isBinary": false
  }
}
