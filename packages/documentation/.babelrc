{
  "presets": [
    "next/babel",
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "extensions": [".js", ".jsx", ".ts", ".tsx"]
    }],
    "inline-react-svg",
    ["prismjs", {
      "languages": [
        "javascript",
        "typescript",
        "jsx",
        "tsx",
        "css",
        "scss",
        "css-extras",
        "markup",
        "bash",
        "diff",
        "git",
        "json",
        "properties"
      ],
      "plugins": [],
      "css": true
    }]
  ]
}
