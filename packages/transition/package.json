{
  "name": "@react-md/transition",
  "version": "2.0.0-alpha-1",
  "description": "A package for working with react-md to use some simple transitions.",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "es/*",
    "lib/*",
    "types/*",
    "dist/*"
  ],
  "scripts": {
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "build:modules": "tsc -p tsconfig.json",
    "build": "npm run types && npm-run-all -p build:commonjs build:modules styles",
    "clean": "rimraf es lib types dist",
    "prebuild": "npm run clean",
    "styles": "cpx \"src/**/*.scss\" dist",
    "watch:styles": "npm run styles -- --watch",
    "watch": "npm-run-all -p \"build:commonjs -- --watch\" \"build:modules -- --watch\" watch:styles",
    "types": "cpx \"src/types.d.ts\" types",
    "test": "jest src",
    "docs": "doc-generator",
    "lint": "npm-run-all -p lint:styles lint:ts",
    "lint:styles": "sass-lint -c ../../.sass-lint.yml -v",
    "lint:ts": "tslint -p tsconfig.json",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": "https://github.com/mlaursen/react-md",
  "keywords": [
    "react-md",
    "material design",
    "react",
    "transition",
    "component",
    "components",
    "collapse"
  ],
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "memoize-one": "^4.0.2",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "doc-generator": "^2.0.0-alpha-1",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  }
}
