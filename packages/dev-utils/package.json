{
  "name": "@react-md/dev-utils",
  "version": "2.0.0-alpha.6",
  "description": "Common build utilities for react-md. Contains helpers for building, watching, testing, etc",
  "private": true,
  "bin": {
    "dev-utils": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rimraf bin dist",
    "executable": "chmod +x bin/cli.js",
    "compile": "tsc",
    "build": "npm-run-all compile executable",
    "watcher": "chokidar \"bin/cli.js\" -c \"npm run executable\"",
    "watch": "npm-run-all -p \"compile -w\" watcher"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "cpx": "^1.5.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "filesize": "^4.1.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "gzip-size": "^5.1.1",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "loglevel": "^1.6.3",
    "markdown-toc": "^1.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "performance-now": "^2.1.0",
    "postcss": "^7.0.17",
    "postcss-combine-duplicated-selectors": "^8.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-sorting": "^5.0.1",
    "prettier": "^1.18.2",
    "pretty-ms": "^5.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.22.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sassdoc": "^2.7.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.2"
  }
}
