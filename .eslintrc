{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
  },
  "rules": {
    "max-len": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "operator-linebreak": 0,
    "prefer-template": 0,
    "wrap-iife": 0,
    "function-paren-newline": 0,
    "implicit-arrow-linebreak": 0,

    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this": 0,
    "react/jsx-filename-extension": 0
  }
}
